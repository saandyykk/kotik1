статическая пустота Main(строка[] аргументов)
        {
 Случайный случайный = новый случайный();
 int[] myArray = новый int[10];

 Консоль.WriteLine("\t 1 массив");
 для (int i = 0; i < мой массив.Длина; i++)
            {
 Мой массив[i]= случайный.Следующий(100);
 Консоль.WriteLine($"{i+1} элемент массива: {MyArray[i]}");
            }
 Консоль.Строка записи("\n");

 int maxInd = Массив.indexOf(myArray, myArray.Max()); 
 int minInd = Массив.indexOf(myArray, myArray.Min());

 int max = myArray.Max();
 int min = myArray.Min();

 Консоль.Строка записи ($"Min[{minInd + 1}]:{myArray.Min()} Max[{maxInd + 1}]:{myArray.Max()}");

 Мой массив[maxInd] = min;
 Мой массив[minInd] = max;

 Консоль.WriteLine("\n\t 2 массив");
 для (int i = 0; i < мой массив.Длина; i++)
            {
 Консоль.WriteLine($"{i + 1} элемент массива: {MyArray[i]}");
            }
        }





















4. Выведите на печать номера элементов заданного массива Y(N), удовлетворяющих условию 0<Y[i]<1
 
процедура TForm1.Button1Click(Отправитель: TObject);
var
 a: массив [1..10] вещественных;
 i:целое число;
начать
Памятка1.Строки.Clear ; // очистка мемо
Memo2.Строки.Очистить ;

для i: = от 1 до 10 сделать
 начать

a[i]:=random*10; // создает рандомные числа в диапазоне 0-1
Памятка1.Строки.Add(FloatToStrF(a[i],ffFixed,0,2));// делает 2 знака после ,
конец;


для i: = от 1 до 10 сделать
 начать

 если (0<a[i]) и (a[i]<1), то 
 Memo2.Строки.Add('элемент: '+FloatToStr(i))

 конец;
конец; 




Программа внутреннего класса
    {
 статическая пустота Main(строка[] аргументов)
        {
 Случайный случайный = новый случайный();
 float[] myArray = новый float[10];

 Консоль.WriteLine("Массив:");
 для (int i = 0; i < мой массив.Длина; i++)
            {
 myArray[i] = (с плавающей точкой) Математика.Раунд(случайный.nextDouble() * 10 , 2);
 Консоль.Написать(myArray[i]+"\t");
            }

 Консоль.WriteLine("\nЭлементы удовлетворяющие условие:");
 для (int i = 0; i < мой массив.Длина; i++)
            {
 если (0 < Мой массив [i] && Мой массив[i] < 1)
                {
 Консоль.WriteLine($"{i+1} элемент ");
                }
            }
 Консоль.Строка записи();
        }

















5. Дан массив A(N). Получите массив B(N), i-й элемент которого равен среднему арифметическому первых i элементов массива.
 Программа внутреннего класса
    {
 статическая пустота Main(строка[] аргументов)
        {
 Случайный случайный = новый случайный();
 int[] MyArrayOld = новый int[5];
 float[] MyArrayNew = новый поплавок[6];
 среднее значение с плавающей запятой = 0;

 Консоль.Строка записи("\t1 Массив\n");
 для (int i = 0; i < MyArrayOld.getLength(0); i++)
            {
 MyArrayOld[i] = случайный.Следующий(3);
 Консоль.Написать(MyArrayOld[i] + "\t");
 СредАр += MyArrayOld[i];
            }
 СредАр /= Мой мир.Получаемая длина(0);
 Консоль.WriteLine($"\n\nCреднеe арифметическоe: {SredAr}\n");

 int j = 0;
 Консоль.Строка записи("\t2 Массив\n");
 для (int i = 0; i < MyArrayNew.Длина; i++)
            {
 если (i==0)
                {
 MyArrayNew[i]= СредАр;
 Консоль.Написать(MyArrayNew[i] + "\t");
 продолжить;
                }

 MyArrayNew[i]= MyArrayOld[j];
 Консоль.Написать(MyArrayNew[i] + "\t");
 j++;
            }

        }

6. Дан целочисленный массив А размера N. Вывести сумму положительных элементов заданного массива A(N).
 
процедура TForm1.Button1Click(Отправитель: TObject);
var
 a: массив [1..10] целых чисел;
 сумма, i:целое число;
начать
Памятка1.Строки.Clear ; // очистка мемо
сумма:=0;
a[1]:=1 ;
a[2]:=12 ;
a[3]:=2 ;
a[4]:=20 ;
a[5]:=-75 ;
a[6]:=-56 ;
a[7]:=-45 ;
a[8]:=35 ;
a[9]:=-45 ;
a[10]:=-542 ;

для i: = от 1 до 10 сделать
 начать
Памятка1.Строки.Добавить(FloatToStr(a[i]));
если a[i]>=0, то
 начать
 сумма:= a[i]+сумма ;
 конец;
конец;
Label1.Заголовок:= 'sum='+ floattostr(сумма);
конец; 


Программа внутреннего класса
    {
 статическая пустота Main(строка[] аргументов)
        {
 Случайный случайный = новый случайный();
 int[] myArray = новый int[10];
 общая сумма = 0;
 для (int i = 0; i < мой массив.Длина; i++)
            {
 Мой массив[i] = случайный.Следующий(-5, 5);
 Консоль.Написать(myArray[i]+"\t");
 если (мой массив[i]>0)
                {
 сумма += мой массив[i];
                }
            }

 Консоль.WriteLine($"\nСумма Положительных элементов = {sum}");
        }
    }
















7. Дан целочисленный массив А размера N. Вывести сумму отрицательных элементов заданного массива A(N).
 
процедура TForm1.Button1Click(Отправитель: TObject);
var
 a: массив [1..10] целых чисел;
 сумма, i:целое число;
начать
Памятка1.Строки.Clear ; // очистка мемо
сумма:=0;
a[1]:=1 ;
a[2]:= 12 ;
a[3]:= 2 ;
a[4]:= 20 ;
a[5]:= -75 ;
a[6]:= -55 ;
a[7]:= -45 ;
a[8]:= 35 ;
a[9]:= -45 ;
a[10]:= -11 ;

для i: = от 1 до 10 сделать
 начать
Памятка1.Строки.Добавить(FloatToStr(a[i]));
если a[i]<=0, то
 начать
 сумма:= a[i]+сумма ;
 конец;
end;
Label1.Caption:= 'sum='+ floattostr(sum);
end;        


internal class Program
    {
        static void Main(string[] args)
        {
            Random random = new Random();
            int[] MyArray = new int[10];
            int sum = 0;
            for (int i = 0; i < MyArray.Length; i++)
            {
                MyArray[i] = random.Next(-5, 5);
                Console.Write(MyArray[i]+"\t");
                if (MyArray[i]<0)
                {
                    sum += MyArray[i];
                }
            }

            Console.WriteLine($"\nСумма отрицательных элементов = {sum}");
        }
    }












8. 	Дан целочисленный массив А размера N. Вывести число отрицательных элементов заданного массива A(N). 
procedure TForm1.Button1Click(Sender: TObject);
var
  a:array [1..10]of integer;
   numberNeg,i:integer;
begin
Memo1.Lines.Clear ; // очистка мемо
numberNeg:=0;
a[1]:=1        ;
a[2]:= 12       ;
a[3]:=  2         ;
a[4]:=  20         ;
a[5]:=  -75         ;
a[6]:=  -55          ;
a[7]:=  -45           ;
a[8]:=    35           ;
a[9]:=    -45           ;
a[10]:=  -11            ;

for i:=1 to 10 do
    begin
Memo1.Lines.Add(FloatToStr(a[i]));
if a[i]<=0 then
   begin
   numberNeg:= numberNeg+1 ;
   end;
end;
Label1.Caption:= 'количество отрицательных ='+ floattostr(numberNeg);
end;    



static void Main(string[] args)
        {
            Random random = new Random();
            int[] MyArray = new int[10];
            int value = 0;
            for (int i = 0; i < MyArray.Length; i++)
            {
                MyArray[i] = random.Next(-5, 5);
                Console.Write(MyArray[i]+"\t");
                if (MyArray[i]<0)
                {
                    value++;
                }
            }

            Console.WriteLine($"\nКоличество Эллементов 0 < : {value}");
        }















9. 	Дан целочисленный массив А размера N. Вывести число положительных элементов заданного массива A(N).
 
procedure TForm1.Button1Click(Sender: TObject);
var
  a:array [1..10]of integer;
   numberPos,i:integer;
begin
Memo1.Lines.Clear ; // очистка мемо
numberPos:=0;
a[1]:=1        ;
a[2]:= 12       ;
a[3]:=  2         ;
a[4]:=  20         ;
a[5]:=  -75         ;
a[6]:=  -55          ;
a[7]:=  -45           ;
a[8]:=    35           ;
a[9]:=    -45           ;
a[10]:=  -11            ;

for i:=1 to 10 do
    begin
Memo1.Lines.Add(FloatToStr(a[i]));
if a[i]>=0 then
   begin
   numberPos:= numberPos+1 ;
   end;
end;
Label1.Caption:= 'Количество положительных='+ floattostr(numberPos);
end;   


internal class Program
    {
        static void Main(string[] args)
        {
            Random random = new Random();
            int[] MyArray = new int[10];
            int value = 0;
            for (int i = 0; i < MyArray.Length; i++)
            {
                MyArray[i] = random.Next(-5, 5);
                Console.Write(MyArray[i]+"\t");
                if (MyArray[i]>0)
                {
                    value++;
                }
            }

            Console.WriteLine($"\nКоличество Эллементов 0 > : {value}");
        }
    }











