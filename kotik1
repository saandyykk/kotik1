1. В заданном массиве A(N) найти произведение наибольшего и наименьшего элементов.
 

процедура TForm1.Button1Click(Отправитель: TObject);
переменная a: массив [1..10] целых чисел;
 min,max,i: целое число;
начать
Памятка1.Строки.Очистить ;
i:=1;

повторить
a[i]:=random(100); // (100) тут задается максимум в рандоме
Памятка1.Линии.Добавить(FloatToStr(a[i]));
если i=1, то
 начать
 максимальное значение := a[i];
 мин:=a[i];
 i:=i+1;
 continue; //закончит текущий повтор цикла
 конец;
если a[i]>max, то
 начать
 Максимальное значение := a[i] ;
 конец;
если a[i]<min, то
 начать
 мин := a[i] ;
 конец;
i:=i+1;
пока i>10 ;

Label1.подпись:= 'max='+floattostr(макс.);
Label2.заголовок:= 'min='+floattostr(min) ;
Label3.заголовок:= 'milt='+floattostr(макс * мин)
конец; 

Программа внутреннего класса
    {
 статическая пустота Main(строка[] аргументов)
        {
 Случайный случайный = новый случайный();
 int[] myArray = новый int[10];

 для (int i = 0; i < мой массив.Длина; i++)
            {
                MyArray[i]= random.Next(10);
                Console.WriteLine($"{i+1} элемент массива: {MyArray[i]}");
            }

 Консоль.Строка записи($"\nПроизведение min & max = {myArray.Max()*myArray.Min()}");
        }
    }


















