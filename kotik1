 Случайный случайный = новый случайный();
 int[] myArray = новый int[10];
 Консоль.Write("Введите число С:");
 с плавающей точкой c = с плавающей точкой.Синтаксический анализ(консоль.Строка чтения());
 условие int = 0;

 для (int i = 0; i < мой массив.Длина; i++)
            {
 Мой массив[i] = случайный.Следующий(0, 50);
 Консоль.Написать(myArray[i]+"\t");
 если (myArray[i]>=c)
                {
 условие++;
                }
            }

 Консоль.WriteLine($"\n\n{condition} болше число С");
